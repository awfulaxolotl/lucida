{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,

    "jsx": "react",
    "lib": ["dom", "es2015", "es2016", "es2017.object"],
    "downlevelIteration": true,

    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
